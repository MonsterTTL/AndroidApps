<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GenerateProjection" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="MySeekBar3" >
        <item name="android:maxHeight">48dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:indeterminateDrawable">?android:attr/colorAccent</item>
        <item name="android:progressDrawable">@drawable/myseekbardraw</item>
        <item name="android:thumb">@color/white</item>

    </style>
</resources>